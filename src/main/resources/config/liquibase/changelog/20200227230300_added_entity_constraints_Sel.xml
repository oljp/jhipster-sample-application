<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Sel.
    -->
    <changeSet id="20200227230300-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="sel_id"
                                 baseTableName="sel_initiating_event"
                                 constraintName="fk_sel_initiating_event_sel_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sel"/>

        <addForeignKeyConstraint baseColumnNames="initiating_event_id"
                                 baseTableName="sel_initiating_event"
                                 constraintName="fk_sel_initiating_event_initiating_event_id"
                                 referencedColumnNames="id"
                                 referencedTableName="distinct_event"/>
        
        <addForeignKeyConstraint baseColumnNames="sel_id"
                                 baseTableName="sel_associated_event"
                                 constraintName="fk_sel_associated_event_sel_id"
                                 referencedColumnNames="id"
                                 referencedTableName="sel"/>

        <addForeignKeyConstraint baseColumnNames="associated_event_id"
                                 baseTableName="sel_associated_event"
                                 constraintName="fk_sel_associated_event_associated_event_id"
                                 referencedColumnNames="id"
                                 referencedTableName="distinct_event"/>
        
    </changeSet>
</databaseChangeLog>
